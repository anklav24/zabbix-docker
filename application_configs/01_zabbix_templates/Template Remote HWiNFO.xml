<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-01-10T18:26:13Z</date>
    <groups>
        <group>
            <name>Templates/Operating systems</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Remote HWiNFO</template>
            <name>Template Remote HWiNFO</name>
            <groups>
                <group>
                    <name>Templates/Operating systems</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>HWiNFO</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>api.ipify.org</name>
                    <type>HTTP_AGENT</type>
                    <key>api.ipify.org</key>
                    <trends>0</trends>
                    <status>DISABLED</status>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>HWiNFO</name>
                        </application>
                    </applications>
                    <url>https://api.ipify.org/</url>
                </item>
                <item>
                    <name>hw.get.hardware</name>
                    <key>hw.get.hardware</key>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>HARDWARE_FULL</inventory_link>
                    <applications>
                        <application>
                            <name>HWiNFO</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED</type>
                            <params/>
                        </step>
                        <step>
                            <type>NOT_MATCHES_REGEX</type>
                            <params>500 Internal Server Error</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                        <step>
                            <type>MATCHES_REGEX</type>
                            <params>^(?!\s*$).+</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1 &#13;
and &#13;
{strlen()}&gt;0</expression>
                            <name>Hardware has changed</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>hw.get.hardware.cpu</name>
                    <type>DEPENDENT</type>
                    <key>hw.get.hardware.cpu</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <status>DISABLED</status>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>HWiNFO</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>CPU.*
\0</params>
                        </step>
                        <step>
                            <type>RTRIM</type>
                            <params>&quot;</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>hw.get.hardware</key>
                    </master_item>
                </item>
                <item>
                    <name>hw.get.hardware.drive</name>
                    <type>DEPENDENT</type>
                    <key>hw.get.hardware.drive</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <status>DISABLED</status>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>HWiNFO</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Drive.*
\0</params>
                        </step>
                        <step>
                            <type>RTRIM</type>
                            <params>&quot;</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>hw.get.hardware</key>
                    </master_item>
                </item>
                <item>
                    <name>hw.get.hardware.motherboard</name>
                    <type>DEPENDENT</type>
                    <key>hw.get.hardware.motherboard</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <status>DISABLED</status>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>HWiNFO</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>[M|m]otherboard.*($|\(.*\)($| ))
\0</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>hw.get.hardware</key>
                    </master_item>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>HWiNFO Hardware discovery</name>
                    <key>hw.get.hardware.lld</key>
                    <delay>1h</delay>
                    <status>DISABLED</status>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#SENSORNAMEUSER}</name>
                            <key>hw.get.hardware.lld[{#SENSORINDEX}]</key>
                            <trends>0</trends>
                            <status>DISABLED</status>
                            <discover>NO_DISCOVER</discover>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>HWiNFO</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>{#SENSORNAMEUSER}
\0</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff()}=1</expression>
                                    <name>HWiNFO: {#SENSORNAMEUSER} has changed</name>
                                    <status>DISABLED</status>
                                    <discover>NO_DISCOVER</discover>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <tags>
                <tag>
                    <tag>Custom</tag>
                </tag>
            </tags>
        </template>
    </templates>
</zabbix_export>
